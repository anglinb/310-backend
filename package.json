    "express": "^4.15.4",
    "express-validation": "^1.0.2",
    "express-validator": "^4.2.0",
    "jsonwebtoken": "^8.0.1",
    "mongorito": "^3.0.4",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "require-dir": "^0.3.2",
    "sinon": "^3.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
